[Unit]
Description=Prepackaged Kubernetes container images
Requires=docker.socket

[Service]
EnvironmentFile=/etc/container-environment
Type=oneshot
ExecStart=/usr/bin/docker-bulkimport --dir=/etc/docker/offline/ --registry-addr=${PLANET_PUBLIC_IP}:5000
#Restart=on-failure
#RestartSec=5

[Install]
WantedBy=multi-user.target
